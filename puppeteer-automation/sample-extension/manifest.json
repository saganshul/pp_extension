{
    "name": "Pikachu Everywhere",
    "version": "0.1",
    "description": "Replace every image with Pikachu images.",
    "manifest_version": 3,
    "web_accessible_resources": [
        {
            "resources": [
                "js/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ]
}
